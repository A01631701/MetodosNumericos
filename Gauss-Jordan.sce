A = [2,-6,-1;
    -3,-1,7;
    -8,1,-2], 
b = [-38;-34;-20]

A_aug = [A b]
a=A_aug
a=rref(a);

x=a(1,4)
y=a(2,4)
z=a(3,4)
//error relativo  

Er1=(-38-(2*x-6*y-1*z))/38
Er2=(-34-(-3*x-1*y+7*z))/34
Er3=(-20-(-8*x+1*y-2*z))/20

Ert=Er1+Er2+Er3
disp(a);
disp(Ert);

